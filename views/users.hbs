<div class="row">
    <div class="col-xs-10">
        <div class="box">
            <div class="box-header">
                <h3 class="box-title">Cities</h3>
            </div>
            <!-- /.box-header -->
            <div class="box-body">
                <!--<a href="/admin/add-college"> <button class="btn btn-primary">Add College</button></a>-->
                <table id="example2" class="table table-bordered table-hover">
                    <thead>
                    <tr>
                        <th>First Name</th>
                        <th>Last Name</th>
                        <th>Email</th>
                        <th>Mobile</th>
                        <th>City</th>
                        <th>College Name</th>
                        <th>Address</th>

                        <th>Gender</th>
                        <th>User Type</th>
                    </tr>
                    </thead>
                    <tbody>
                    {{#if Users}}
                        {{#each Users}}
                            <tr>

                                <td>{{this.first_name}}
                                </td>
                                <td>{{this.last_name}}</td>
                                <td>{{this.email}}</td>
                                <td>{{this.mobile}}</td>
                                <td>{{this.city_name}}</td>
                                <td>{{this.college_name}}</td>
                                <td>{{this.college_address}}</td>
                                <td>{{this.gender}}</td>
                                <td>{{this.user_type}}</td>
                                <td><a href="/admin/delete-user/{{this.user_id}}">DELETE</a> </td>


                            </tr>
                        {{/each}}
                    {{/if}}


                    </tbody>

                </table>
            </div>
            <!-- /.box-body -->
        </div>
        <!-- /.box -->
    </div>
</div>