<div class="row">
    <div class="col-xs-12">
        <div class="box">
            <div class="box-header">
                <h3 class="box-title">Cities</h3>
            </div>
            <!-- /.box-header -->
            <div class="box-body">
               <!-- <a href="/admin/add-college"> <button class="btn btn-primary">Add College</button></a>-->
                <table id="example2" class="table table-bordered table-hover">
                    <thead>
                    <tr>
                        <th>Trip ID</th>
                        <th>Date</th>
                        <th>Time</th>
                        <th>Vehicle</th>
                        <th>Seats</th>
                        <th>Vehicle Details</th>
                        <th>Rules</th>
                        <th>Origin</th>
                        <th>Destination</th>
                        <th>Driver ID</th>
                        <th>Status</th>

                    </tr>
                    </thead>
                    <tbody>
                    {{#if Trips}}
                        {{#each Trips}}
                            <tr>
                                <td>{{this.trip_id}}</td>
                                <td>{{this.date}}
                                </td>
                                <td>{{this.time}}</td>
                                <td>{{this.vehicle}}</td>
                                <td>{{this.seats}}</td>
                                <td>{{this.v_details}}</td>
                                <td>{{this.rules}}</td>
                                <td>{{this.origin}}</td>
                                <td>{{this.destination}}</td>
                                <td>{{this.user}}</td>
                                <td>{{this.status}}</td>

                            </tr>
                        {{/each}}
                    {{/if}}

                    </tbody>

                </table>
            </div>
            <!-- /.box-body -->
        </div>
        <!-- /.box -->
    </div>
</div>